<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
          xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <servers>
        <server>
            <id>central</id>
            <username>${env.MAVEN_REPO_USER}</username>
            <password>${env.MAVEN_REPO_PASS}</password>
        </server>

        <server>
            <id>ebi-repo</id>
            <username>${env.ebi_repo_user}</username>
            <password>${env.ebi_repo_passwd}</password>
        </server>
        <server>
            <id>ebi-repo-snapshots</id>
            <username>${env.ebi_repo_user}</username>
            <password>${env.ebi_repo_passwd}</password>
        </server>
        <server>
            <id>nexus-ebi-repo</id>
            <username>${env.nexus_ebi_repo_user}</username>
            <password>${env.nexus_ebi_repo_passwd}</password>
        </server>
        <server>
            <id>nexus-ebi-repo-snapshots</id>
            <username>${env.nexus_ebi_repo_user}</username>
            <password>${env.nexus_ebi_repo_passwd}</password>
        </server>
        <server>
            <id>pst-release</id>
            <username>${env.pst_repo_user}</username>
            <password>${env.pst_repo_passwd}</password>
        </server>
        <server>
            <id>pst-snapshots</id>
            <username>${env.pst_repo_user}</username>
            <password>${env.pst_repo_passwd}</password>
        </server>
    </servers>
</settings>